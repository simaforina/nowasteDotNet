@model IList<Nowaste.Models.ClientRequestViewModels>
@{
    ViewBag.Title = "Мои заявки";
}
<table>
    <tr>
        <td style="border: 1px solid #67b23d; padding-left:10px">
            
            @Html.Partial("_PartialNavList")
        </td>
        <td style="padding-left:20px;">
            <div style="margin-left:50px; margin-top:17px;">МOИ ЗАЯВКИ</div>
            <table>
                <tr style="padding:20px;">
                    <th style="padding:20px;">Дата заявки</th>
                    <th style="padding:20px;">Дата вывоза</th>
                    <th style="padding:20px;">Вторресурс</th>
                    <th style="padding:20px;">Вес</th>
                    <th style="padding:20px;">Статус заявки</th>
                </tr>
                @if (Model.Count != 0)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td style="padding:10px;">@item.RequestDate</td>
                            <td style="padding:10px;">@item.TakeOutDate</td>
                            <td style="padding:10px;">@item.RecycleResourse</td>
                            <td style="padding:10px;">@item.RecycleWeight</td>
                            <td style="padding:10px;">@item.Status</td>

                        </tr>
                    }
                }
            </table>
        </td>
</tr></table>